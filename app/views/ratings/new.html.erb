<%= form_for(@rating) do |f| %>
  <div class="field">
    <%= f.label :score %><br>
    <%= f.number_field :score %>
  </div>
  <div class="field">
    <%= f.label :post_id %><br>
    <%= f.select :post_id, options_from_collection_for_select(@posts, :id, :title ) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
